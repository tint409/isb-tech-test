    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>25cad06c-dca6-47fc-8e49-1b52c04f61e7</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>.</DockerfileContext>
